/dts-v1/;

#include "mt7628an.dtsi"

/ {
	compatible = "mediatek,mt7628an-eval-board", "mediatek,mt7628an-soc";
	model = "Mediatek MT7628AN evaluation board";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "jtag", "perst";
				ralink,function = "gpio";
			};
		};
	};

	wmac@10300000 {
		status = "okay";
	};

	palmbus@10000000 {
		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "jedec,spi-nor";
				reg = <0 0>;
				linux,modalias = "m25p80", "en25q64";
				spi-max-frequency = <10000000>;
				m25p,chunked-io = <32>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
					read-only;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xfb0000>;
				};
			};
		};

//rak seven
		i2c@900 {

            status = "okay";

            zl380tw@45 {
                compatible = "microsemi,zl380tw-codec";
                reg = <0x45>;
            };
            zl38067tw@52 {
                compatible = "microsemi,zl38067tw-codec";
                reg = <0x52>;
            };
		};
	};
};
